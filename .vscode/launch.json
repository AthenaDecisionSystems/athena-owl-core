        {
            "name": "Python Debugger:LOAN project",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": { "PYTHONPATH": "${workspaceRoot}/owl-agent-backend/src:${workspaceRoot}/../athena-owl-demos/IBM-MiniLoan-demo/ibu_backend/src"},
            //"cwd": "${workspaceRoot}/owl-agent-backend"
            "cwd": "${workspaceRoot}/../athena-owl-demos/IBM-MiniLoan-demo/ibu_backend/"
